    "project_key": "{{cookiecutter.project_name|lower|replace('-', '')|replace('_', '')}}",
    "base_package": "dev.mbo",
    "base_package_dir": "{{cookiecutter.base_package|replace('.', '/')}}",
    "package": "{{cookiecutter.base_package}}.{{cookiecutter.project_key}}",
